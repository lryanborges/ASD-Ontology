:PersonWithASD owl:equivalentClass [
  rdf:type owl:Class;
  owl:unionOf (:YouthWithASD :ElderlyPersonWithASD :AdultWithASD :ChildWithASD)
].
[ rdf:type owl:AllDisjointClasses ] owl:members (:WomanWithASD :ManWithASD).
:PersonWithASD owl:equivalentClass [
  rdf:type owl:Class;
  owl:unionOf (:WomanWithASD :ManWithASD)
].
:patientHasMaleTendency rdf:type owl:ObjectProperty;
    rdfs:domain :Patient;
    rdfs:range :MaleTendency;
    rdf:type gufo:MaterialRelationshipType.
:patientHasFemaleTendency rdf:type owl:ObjectProperty;
    rdfs:domain :Patient;
    rdfs:range :FemaleTendency;
    rdf:type gufo:MaterialRelationshipType.
